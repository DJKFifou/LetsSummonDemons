{
  "dependencies": {
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "tslib": "~2.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "~29.5",
    "@types/lodash": "^4.14.201",
    "@types/node": "~18",
    "@typescript-eslint/eslint-plugin": "~6.2",
    "@typescript-eslint/parser": "~6.2",
    "eslint": "~8.46",
    "eslint-config-prettier": "~9.0",
    "eslint-plugin-jest": "~27.2",
    "husky": "^8.0.0",
    "jest": "~29.6",
    "prettier": "~3.0",
    "rimraf": "~5.0",
    "tailwindcss": "^3.4.14",
    "ts-api-utils": "~1.0",
    "ts-jest": "~29.1",
    "typescript": "~5.1"
  },
  "engines": {
    "node": ">= 18.12 <=19"
  },
  "name": "lsd",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "workspaces": [
    "./back",
    "./front"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . --ext .ts --ext .js --ext .mjs --ext .mts --ext tsx --ext jsx",
    "prettier": "prettier --config .prettierrc --write .",
    "dev": "concurrently --names \"BUILD,BACK,FRONT,TAILWIND\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgCyan.bold\" \"npm run build:watch -w back\" \"npm run dev -w back\" \"npm run dev -w front\" \"npm run tailwind:watch\"",
    "tailwind:build": "tailwindcss -i ./front/app/globals.css -o ./front/dist/globals.css --minify",
    "tailwind:watch": "tailwindcss -i ./front/app/globals.css -o ./front/dist/globals.css --watch"
  }
}
